---
type Item = {
  title: string;
  link: string;
};

type Props = {
  items: Item[];
  classes?: string;
};

const { items, classes } = Astro.props;
---

<div class={classes}>
  <ul>
    {
      items?.map((item) => (
        <li>
          <a href={item.link}>{item.title}</a>
        </li>
      ))
    }
  </ul>
</div>
